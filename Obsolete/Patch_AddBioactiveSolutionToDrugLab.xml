<?xml version="1.0" encoding="utf-8" ?>

<!-- Deletes the VAE research projects and adds a new project to incorporate those that aren't consoliated into vanilla projects. -->
<Patch>
  <Operation Class="PatchOperationAdd" MayRequire="ludeon.rimworld.ideology">
    <xpath>/Defs/ThingDef [defName="DrugLab"]</xpath>
    <value>
      <recipes>
        <li>Make_BioactiveSolution</li>
      </recipes>
    </value>
  </Operation>
</Patch>